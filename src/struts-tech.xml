<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<constant name="struts.custom.i18n.resources" value="globalMessages"></constant>
	<package name="techModel" extends="json-default" >
		<action name="techTask" class="techTaskAction">
			<!-- by xjw start -->
			<result name="assignPage">/jsp/techtrans/taskAssignPage.jsp</result>
			<result name="assignPrePage">/jsp/techtrans/taskAssignPrePage.jsp</result>
			<result name="worktimeMaitain">/jsp/techtrans/techWorktimeMaitain.jsp</result>
			<result name="worktimeVerify">/jsp/techtrans/techWorktimeVerify.jsp</result>
			<result name="taskSearch">/jsp/techtrans/techTaskSearch.jsp</result>
			<result name="changeTypeReport">/jsp/techtrans/techChangeTypeReport.jsp</result>
			<result type="json">
				<param name="root">result</param>
			</result>
			<!-- by xjw end -->
			
			<!-- by yk start -->
			<result name="techTaskInfoPage">/jsp/techtrans/techTaskInfoPage.jsp</result>
			<result name="techTaskMaterialCheck">/jsp/techtrans/techTaskMaterialCheck.jsp</result>
			<result name="techTaskMaterialCheckPage">/jsp/techtrans/techTaskMaterialCheckPage.jsp</result>
			<result name="techTaskCollect">/jsp/techtrans/techTaskCollect.jsp</result>
			<result name="techTaskReport">/jsp/techtrans/techTaskReport.jsp</result>
			<!-- by yk end -->
			
			<!-- by wx start -->
			<result name="taskMaintainPage">/jsp/techtrans/techTaskMaintain.jsp</result>
			<result name="workHourEstimatePage">/jsp/techtrans/techWorkHourEstimate.jsp</result>
			<result name="followingUpPage">/jsp/techtrans/techFollowingUp.jsp</result>
			<!-- by wx end -->	
		</action>
		
	</package>
</struts>	
		